{
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": true
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.type.class",
          "keyword",
          "storage.modifier",
          "storage.type.class.js",
          "storage.type.function.js",
          "storage.type.js",
          "keyword.control.import.js",
          "keyword.control.from.js",
          "keyword.control.flow.js",
          "keyword.control.conditional.js",
          "keyword.control.loop.js",
          "keyword.control.sql",
          "keyword.operator.new.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json",
          "entity.name.type.class.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer",
    "editor.formatOnSave": true
},
  "workbench.startupEditor": "newUntitledFile",
  "php-cs-fixer.rules": "@PhpCsFixer",
  "php-cs-fixer.lastDownload": 1680205315848,
  "php.suggest.basic": false,
  "files.associations": {
    "*.http": "http"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.confirmSync": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "#",
      "color": "#ffff00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "$",
      "color": "#557355",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "?",
      "color": "#00d1e0",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "/",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "~",
      "color": "#474747",
      "strikethrough": true,
      "backgroundColor": "transparent"
    },
    {
      "tag": "*",
      "color": "#EEEEEE",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "workbench.statusBar.visible": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.fontLigatures": true,
  "rest-client.enableTelemetry": false,
  "terminal.integrated.fontFamily": "MesloLGS NF,FuraMono Nerd Font Regular, FuraMono Nerd Font",
  "breadcrumbs.enabled": true,
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "editor.largeFileOptimizations": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "phpmd.verbose": true,
  "debug.allowBreakpointsEverywhere": true,
  "json.maxItemsComputed": 1000000,
  "projectManager.any.baseFolders": [
    "~/Documents/Construshop/Repos",
    "~/Documents/Projects"
  ],
  "projectManager.any.maxDepthRecursion": 1,
  "gitlens.views.commits.files.layout": "list",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.maxTokenizationLineLength": 200000,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "phpmd.rules": "cleancode,codesize,design,unusedcode",
  "php.validate.enable": false,
  "emmet.excludeLanguages": ["markdown", "php"],
  "intelephense.environment.phpVersion": "7.4",
"projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.": "esbenp.prettier-vscode",
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "editor.fontFamily": "'Firicico', 'Droid Sans Mono', 'monospace', monospace",
  "workbench.iconTheme": "material-icon-theme",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "merge-conflict.diffViewPosition": "Beside",
  "editor.stickyScroll.enabled": true,
  "gitlens.views.stashes.files.layout": "list",
  "markdown-pdf.includeDefaultStyles": true,
  "markdown-pdf.styles": [
    "markdown-pdf.css"
  ],
  "markdown-pdf.displayHeaderFooter": false,
  "markdown-pdf.highlight": false,
  "markdown-pdf.printBackground": false,
  "markdown-pdf.stylesRelativePathFile": true,
  "php-cs-fixer.executablePath": "php-cs-fixer",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "gitlens.views.branches.branches.layout": "list",
  "todomd.defaultFile": "~/Documents/Miguel/todo.txt",
  "workbench.editor.untitled.hint": "hidden",
  "terminal.external.linuxExec": "zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "codeium.enableConfig": {
    "*": true,
    "c": false
  },
  "terminal.integrated.env.linux": {},
  "totvsLanguageServer.reconnectLastServer": false,
  "totvsLanguageServer.showBanner": false,
  "totvsLanguageServer.welcomePage": false,
  "[nginx]": {
    "editor.defaultFormatter": "raynigon.nginx-formatter"
  },
  "html.format.indentInnerHtml": true,
  "html.format.maxPreserveNewLines": 0,
  "window.menuBarVisibility": "toggle",
  "workbench.colorTheme": "Dracula Soft",
}